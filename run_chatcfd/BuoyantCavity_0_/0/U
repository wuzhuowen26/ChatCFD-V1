FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    hot
    {
        type            noSlip;
    }
    cold
    {
        type            noSlip;
    }
    topAndBottom
    {
        type            noSlip;
    }
    frontAndBack
    {
        type            noSlip;
    }
}


