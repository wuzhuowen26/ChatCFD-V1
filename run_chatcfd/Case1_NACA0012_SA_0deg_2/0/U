FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (52.08 0 0);

boundaryField
{
    airfoil
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    inlet
    {
        type            freestreamVelocity;
        freestreamValue uniform (52.08 0 0);
    }
    outlet
    {
        type            freestreamVelocity;
        freestreamValue uniform (52.08 0 0);
    }
}