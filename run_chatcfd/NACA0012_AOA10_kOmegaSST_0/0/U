FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}

dimensions [0 1 -1 0 0 0 0];

internalField   uniform (52.08 0 0);

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform (52.08 0 0);
    }
    outlet
    {
        type            fixedValue;
        value           uniform (52.08 0 0);
    }
    airfoil
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
}