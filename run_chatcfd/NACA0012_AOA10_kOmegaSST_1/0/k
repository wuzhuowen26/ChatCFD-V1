FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.1;

boundaryField
{
    airfoil
    {
        type            kqRWallFunction;
        value           uniform 0.1;
    }

    inlet
    {
        type            freestream;
        freestreamValue uniform 0.1;
    }

    outlet
    {
        type            freestream;
        freestreamValue uniform 0.1;
    }
}
