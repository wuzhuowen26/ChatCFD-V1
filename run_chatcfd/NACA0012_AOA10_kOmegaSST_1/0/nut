FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 2.45e-2;

boundaryField
{
    airfoil
    {
        type            nutkWallFunction;
        value           uniform 0;
    }

    inlet
    {
        type            calculated;
        value           uniform 2.45e-2;
    }

    outlet
    {
        type            calculated;
        value           uniform 2.45e-2;
    }
}