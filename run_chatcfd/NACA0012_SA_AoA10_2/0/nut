    inlet
    {
        type            freestream;
        freestreamValue uniform 8.7e-6;
    }
    outlet
    {
        type            freestream;
        freestreamValue uniform 8.7e-6;
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
}