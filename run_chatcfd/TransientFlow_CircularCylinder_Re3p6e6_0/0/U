FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (3.6 0 0);

boundaryField
{
    cylinder
    {
        type            symmetry;
    }
    left
    {
        type            fixedValue;
        value           uniform (3.6 0 0);
    }
    right
    {
        type            zeroGradient;
    }
    up
    {
        type            symmetry;
    }
    down
    {
        type            symmetry;
    }
    defaultFaces
    {
        type            empty;
    }
}