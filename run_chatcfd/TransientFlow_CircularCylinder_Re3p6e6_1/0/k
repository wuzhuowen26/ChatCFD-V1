FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      k;
}

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0.01142;

boundaryField
{
    left
    {
        type            fixedValue;
        value           uniform 0.01142;
    }

    right
    {
        type            zeroGradient;
    }

    up
    {
        type            symmetry;
    }

    down
    {
        type            symmetry;
    }

    cylinder
    {
        type            symmetry;
    }

    defaultFaces
    {
        type            empty;
    }
}