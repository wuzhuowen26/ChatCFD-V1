FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (3.6 0 0);

boundaryField
{
    left
    {
        type            fixedValue;
        value           uniform (3.6 0 0);
    }
    right
    {
        type            inletOutlet;
        inletValue      uniform (3.6 0 0);
        phi             phi;
        value           uniform (3.6 0 0);
    }
    up
    {
        type            symmetryPlane;
    }
    down
    {
        type            symmetryPlane;
    }
    cylinder
    {
        type            noSlip;
        value           uniform (0 0 0);
    }
    defaultFaces
    {
        type            empty;
    }
}