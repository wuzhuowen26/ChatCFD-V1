    {
        type            fixedValue;
        value           uniform 0.01142;
    }
    right
    {
        type            fixedValue;
        value           uniform 0.01142;
    }
    up
    {
        type            fixedValue;
        value           uniform 0.01142;
    }
    defaultFaces
    {
        type            zeroGradient;
    }
}
