FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon;
}

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.001568;

boundaryField
{
    left        { type turbulentMixingLengthDissipationRateInlet; mixingLength 0.07; value uniform 0.001568; }
    right       { type zeroGradient; }
    up          { type zeroGradient; }
    down        { type zeroGradient; }
    cylinder    { type epsilonWallFunction; value uniform 0.001568; }
    defaultFaces { type empty; }
}
