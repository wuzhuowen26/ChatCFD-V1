FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      epsilon;
}

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 0.0067;

boundaryField
{
    cylinder
    {
        type            epsilonWallFunction;
        value           uniform 0.0067;
    }
    defaultFaces
    {
        type            empty;
    }
    down
    {
        type            inletOutlet;
        inletValue      uniform 0.0067;
        value           uniform 0.0067;
    }
    right
    {
        type            inletOutlet;
        inletValue      uniform 0.0067;
        value           uniform 0.0067;
    }
    up
    {
        type            inletOutlet;
        inletValue      uniform 0.0067;
        value           uniform 0.0067;
    }
    left
    {
        type            fixedValue;
        value           uniform 0.0067;
    }
}
