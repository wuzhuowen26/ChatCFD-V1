FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    cylinder
    {
        type            symmetry;
    }

    down
    {
        type            symmetry;
    }

    right
    {
        type            zeroGradient;
    }

    up
    {
        type            symmetry;
    }

    left
    {
        type            zeroGradient;
    }

    defaultFaces
    {
        type            empty;
    }
}

